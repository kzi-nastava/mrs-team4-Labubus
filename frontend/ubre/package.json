{
  "name": "ubre",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --include=\"**/*.spec.ts\" --browsers=ChromeHeadless",
    "test:ci:pass": "ng test --no-watch --include=\"**/driver-registration-service.spec.ts\" --include=\"**/user-layout.spec.ts\" --browsers=ChromeHeadless",
    "test:bogdan": "ng test --no-watch --include=\"**/driver-registration-service.spec.ts\" --include=\"**/user-layout.spec.ts\" --browsers=ChromeHeadless"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@stomp/stompjs": "^7.2.1",
    "canvas-confetti": "^1.9.4",
    "chart.js": "^4.5.1",
    "event-source-polyfill": "^1.0.31",
    "leaflet": "^1.9.4",
    "leaflet-rotatedmarker": "^0.2.0",
    "leaflet.marker.slideto": "^0.3.0",
    "rxjs": "~7.8.0",
    "sockjs-client": "^1.6.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/jasmine": "^5.1.4",
    "@types/leaflet": "^1.9.21",
    "@types/sockjs-client": "^1.5.4",
    "jasmine-core": "^5.4.0",
    "jsdom": "^27.1.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.9.2"
  }
}
